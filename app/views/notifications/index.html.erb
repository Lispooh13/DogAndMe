<% notifications = @notifications.where.not(visitor_id: current_user.id) %><!--自分の投稿に対するいいね、コメントは通知に表示しない-->
<% if notifications.exists? %>
  <%= render notifications %>
  <%= paginate notifications %>
<% else %>
<div class="row">
  <div class="col-12 m-4">
    <p>通知はありません</p>
  </div>
</div>
<% end %>